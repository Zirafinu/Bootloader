{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "gdb",
      "request": "launch",
      "name": "Debug CTest",
      "cwd": "${cmake.testWorkingDirectory}",
      "program": "${cmake.testProgram}",
      "args": ["${cmake.testArgs}"],
      "initCommands": ["break main"]
    },
    {
      "name": "Debug with OpenOCD",
      "type": "cortex-debug",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceFolder}/examples/build/Debug/stm32f407/stm32f407_bootloader.elf",
      "loadFiles": [
        "${workspaceFolder}/examples/build/Debug/stm32f407/stm32f407_bootloader.elf",
        "${workspaceFolder}/examples/build/Debug/stm32f407/blinky.elf"
      ],
      "symbolFiles": [
        "${workspaceFolder}/examples/build/Debug/stm32f407/stm32f407_bootloader.elf",
        "${workspaceFolder}/examples/build/Debug/stm32f407/blinky.elf"
      ],
      "toolchainPrefix": "arm-none-eabi",
      "gdbPath": "gdb-multiarch",
      "servertype": "openocd",
      "configFiles": ["board/stm32f4discovery.cfg"],
      "device": "STM32F407VG6",
      "svdFile": "${workspaceFolder}/examples/stm32f407/STM32F407.svd",
      "runToEntryPoint": "main",
      "showDevDebugOutput": "none"
    }
  ]
}
