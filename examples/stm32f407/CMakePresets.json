{
  "version": 8,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 28,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "default",
      "hidden": true,
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "toolchainFile": "cube/arm_none_eabi.cmake"
    },
    {
      "name": "Release",
      "inherits": "default",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "ON_TARGET": "1"
      }
    },
    {
      "name": "Debug_in_qemu",
      "inherits": "default",
      "toolchainFile": "qemu/arm_none_eabi.cmake",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "ON_TARGET": "0"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "Release",
      "configurePreset": "Release"
    },
    {
      "name": "Debug_in_qemu",
      "configurePreset": "Debug_in_qemu"
    }
  ],
  "testPresets": [
    {
      "name": "Release",
      "configurePreset": "Release",
      "output": {
        "outputOnFailure": true
      }
    },
    {
      "name": "Debug_in_qemu",
      "configurePreset": "Debug_in_qemu",
      "output": {
        "outputOnFailure": true
      }
    }
  ],
  "workflowPresets": [
    {
      "name": "Release",
      "steps": [
        {
          "type": "configure",
          "name": "Release"
        },
        {
          "type": "build",
          "name": "Release"
        },
        {
          "type": "test",
          "name": "Release"
        }
      ]
    },
    {
      "name": "Debug_in_qemu",
      "steps": [
        {
          "type": "configure",
          "name": "Debug_in_qemu"
        },
        {
          "type": "build",
          "name": "Debug_in_qemu"
        },
        {
          "type": "test",
          "name": "Debug_in_qemu"
        }
      ]
    }
  ]
}
